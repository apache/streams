{
  "type": "object",
  "$schema": "http://json-schema.org/draft-03/schema",
  "$license": [
    "http://www.apache.org/licenses/LICENSE-2.0"
  ],
  "id": "#",
  "javaType": "org.apache.streams.pipl.pojo.Url",
  "properties": {
    "@source_id": {
      "type": "string",
      "description": "The source ID (if this URL is a source of data)"
    },
    "@domain": {
      "type": "string",
      "description": "The domain"
    },
    "@name": {
      "type": "string",
      "description": "The site name."
    },
    "@category": {
      "type": "string",
      "description": "One of: 'background_reports’, 'contact_details’, 'email_address’, 'media’, 'personal_profiles’, 'professional_and_business’, 'public_records’, 'publications’, 'school_and_classmates’, 'web_pages’"
    },
    "url": {
      "type": "string",
      "description": "The actual URL"
    },
    "@sponsored": {
      "type": "boolean",
      "description": "Indicating whether additional data from this source is available behind a website paywall. Omitted if false."
    }
  }
}