{
    "type": "object",
    "$schema": "http://json-schema.org/draft-03/schema",
    "javaType": "org.apache.streams.datasift.twitter.DatasiftTwitterUser",
    "properties": {
        "created_at": {
            "type": "string",
            "format": "date-time"
        },
        "description": {
            "type": "string"
        },
        "favourites_count": {
            "type": "integer"
        },
        "followers_count": {
            "type": "integer"
        },
        "friends_count": {
            "type": "integer"
        },
        "geo_enabled": {
            "type": "boolean"
        },
        "id": {
            "type": "integer"
        },
        "id_str": {
            "type": "string"
        },
        "lang": {
            "type": "string"
        },
        "listed_count": {
            "type": "integer"
        },
        "location": {
            "type": "string"
        },
        "name": {
            "type": "string"
        },
        "profile_image_url": {
            "type": "string"
        },
        "profile_image_url_https": {
            "type": "string"
        },
        "screen_name": {
            "type": "string"
        },
        "statuses_count": {
            "type": "integer"
        },
        "time_zone": {
            "type": "string"
        },
        "url": {
            "type": "string"
        },
        "utc_offset": {
            "type": "integer"
        },
        "verified": {
            "type": "boolean"
        }
    }
}