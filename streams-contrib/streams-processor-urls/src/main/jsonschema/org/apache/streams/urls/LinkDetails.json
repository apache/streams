{
    "type": "object",
    "$schema": "http://json-schema.org/draft-03/schema",
    "id": "#",
    "javaType": "org.apache.streams.urls.LinkDetails",
    "javaInterfaces": ["java.io.Serializable"],
    "properties": {
        "linkStatus" : {
            "type" : "string",
            "enum" : [
                "SUCCESS",
                "ERROR",
                "TIME_OUT",
                "MALFORMED_URL",
                "NOT_FOUND",
                "FORBIDDEN",
                "REDIRECT_ERROR",
                "UNAUTHORIZED",
                "LOOP",
                "HTTP_ERROR_STATUS",
                "EXCEPTION",
                "EMPTY",
                "TOO_LARGE",
                "NONE"
            ],
            "default" : "NONE"
        },
        "originalURL": {
            "type": "string",
            "default" : ""
        },
        "finalURL": {
            "type": "string",
            "default" : ""
        },
        "contentType" : {
            "type" : "string",
            "default" : ""
        },
        "normalizedURL": {
            "type": "string",
            "default" : ""
        },
        "urlParts": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "domain": {
            "type": "string",
            "default" : ""
        },
        "redirects": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "tracked": {
            "type": "boolean",
            "default" : "false"
        },
        "finalResponseCode" : {
            "type" : "integer",
            "default" : "-1"
        },
        "cookies": {
            "type": "array",
            "uniqueItems": true,

            "items": {
                "type": "string"
            }
        },
        "startTime": {
            "type": "string",
            "format" : "date-time"
        },
        "tookInMills": {
            "type": "integer"
        },
        "contentsType" : {
            "type" : "string",
            "enum" : [
                "RAW_HTML",
                "BASE64"
            ]
        },
        "contents": {
            "type": "string"
        }
    }
}