{
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "$license": [
    "http://www.apache.org/licenses/LICENSE-2.0"
  ],
  "id": "#",
  "javaType": "org.apache.streams.peopledatalabs.pojo.EducationItem",
  "properties": {
    "school": {
      "type": "object",
      "javaType": "org.apache.streams.peopledatalabs.pojo.School",
      "properties": {
        "name": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "location": {
          "type": "string"
        },
        "profiles": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "website": {
          "type": "string"
        }
      }
    },
    "end_date": {
      "type": "string"
    },
    "start_date": {
      "type": "string"
    },
    "gpa": {
      "type": "number"
    },
    "degrees": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "majors": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "minors": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "locations": {
      "type": "array",
      "items": {
        "$ref": "./Location.json"
      }
    }
  }
}
